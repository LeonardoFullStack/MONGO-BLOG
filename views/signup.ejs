<%- include('partials/header.ejs') %>
<main>
<h1>
    <%= title %>
</h1>

<p>
    <%= msg %>
</p>
<form method="post" action="/signup" class="signupForm" enctype="multipart/form-data">
    <label for="name">Nickname</label>
    <input type="text" name="name" placeholder="nombre">
    <label for="surname">Nombre</label>
    <input type="text" name="surname" placeholder="Pseudónimo">
    <label for="email">E-mail</label>
    <input type="text" name="email" placeholder="e-mail">
    <label for="avatar">Tu avatar</label>
    <input type="file" name="avatar">
    <label for="password">Contraseña</label>
    <input type="password" name="password" placeholder="contraseña">
    <input type="submit" value="Quiero entrar!">

</form>
</main>
<%- include('partials/footer.ejs') %>